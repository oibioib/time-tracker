{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "tabWidth": 2,
  "arrowParens": "always",
  "bracketSameLine": true,
  "importOrder": [
    "^@core/(.*)$",
    "^@server/(.*)$",
    "^@mui/(.*)$",
    "^@ui/(.*)$",
    "^[./](?!.*?\\.css$).*$",
    "(.*)css$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
